	[68, 9, 64, 150, -31, -119, filename+".png"]
	[134, 9, 64, 150, -31, -119, filename+".png"]
	[200, 9, 64, 150, -31, -119, filename+".png"]
	[266, 9, 64, 150, -31, -119, filename+".png"]
	[332, 9, 64, 150, -31, -119, filename+".png"]
	[398, 9, 64, 150, -31, -119, filename+".png"]
	[2, 51, 64, 58, -31, -26, filename+".png"]		//purchase menu graphic (6.)
	[68, 9, 64, 150, -31, -119, filename+".png"]	
	[68, 9, 64, 150, -31, -119, filename+".png"]
	[68, 9, 64, 150, -31, -119, filename+".png"]			//fake frames (3 times multiplied 1.) due to:
	[68, 9, 64, 150, -31, -119, filename+"_animation.png"]	//special animation frames (starting from 10.)
	[134, 9, 64, 150, -31, -119, filename+"_animation.png"]
	[200, 9, 64, 150, -31, -119, filename+"_animation.png"]
	[266, 9, 64, 150, -31, -119, filename+"_animation.png"]
	[332, 9, 64, 150, -31, -119, filename+"_animation.png"]
	[398, 9, 64, 150, -31, -119, filename+"_animation.png"]
}
