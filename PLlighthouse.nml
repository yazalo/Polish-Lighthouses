grf {
	grfid: "yz\00\00";		//797A0000
	name: string(STR_GRF_NAME);
	desc: string(STR_GRF_DESCRIPTION);
	version: 3;
	min_compatible_version: 3;
	url: string(STR_GRF_URL);
	param {
		param_only_se {
		type:    	bool;
		name:    	string(STR_ONLY_SE);
		desc:    	string(STR_ONLY_SE_DESC);
		def_value:	0;
		}
	}
	param {
		param_irremovable {
		type:    	int;
		name:    	string(STR_IRREMOVABLE);
		desc:    	string(STR_IRREMOVABLE_DESC);	
		max_value:	2;
        def_value:	0;		
 			names: {
 				0:	string(STR_IRREMOVABLE_OFF);
				1:	string(STR_IRREMOVABLE_EXISTING);
 				2:	string(STR_IRREMOVABLE_ALL);
 			};
		}
	}
	param {
		param_always_av {
		type:    	bool;	
		name:    	string(STR_ALWAYS_AV);
		desc:    	string(STR_ALWAYS_AV_DESC);
        def_value:	0;
		}
	}
	/*
	
	param {
		param_rebuilt_lh {
		type:    int;
		name:    string(STR_REBUILT_LHS);
		desc:    string(STR_REBUILT_LHS_DESC);
		max_value: 1;
        def_value: 1;			
 			names: {
 				0: string(STR_AS_SEPARATE_OBJECTS);
 				1: string(STR_REBUILD_AUTOMATICALLY);
 			};
		}
	}*/
	param {
		param_fixed_anim {
		type:    	int;	
		name:    	string(STR_FIXED_ANIM);
		desc:    	string(STR_FIXED_ANIM_DESC);
		max_value: 1;
        def_value: 0;			
 			names: {
 				0: string(STR_FIXED_ANIM_FIXED);
 				1: string(STR_FIXED_ANIM_ANIM);
 			};
		}
	}
	param {
		param_anim_base_speed {
		name:		string(STR_UNIFIED_ANIM_SPEED);
		desc:    	string(STR_UNIFIED_ANIM_SPEED_DESC);
		type:		bool;
        def_value:	0;
		}
	}
	param {
		param_anim_speed {
		name:		string(STR_ANIM_SPEED);
		desc:    	string(STR_ANIM_SPEED_DESC);
		type:		int;
		min_value:	2;
		max_value:	6;
        def_value:	4;
			names: {
 				0:	string(STR_ANIM_SPEED_1_16);
 				1:	string(STR_ANIM_SPEED_1_8);
 				2:	string(STR_ANIM_SPEED_1_4);
 				3:	string(STR_ANIM_SPEED_1_2);
 				4:	string(STR_ANIM_SPEED_1);
 				5:	string(STR_ANIM_SPEED_2);
 				6:	string(STR_ANIM_SPEED_4);
 				7:	string(STR_ANIM_SPEED_8);
 				8:	string(STR_ANIM_SPEED_16);
 				9:	string(STR_ANIM_SPEED_32);
			};
		}
	}
}
